<h1>投稿一覧</h1>
<% @posts.each do |post| %>
  <div class="post">
    <h2>おうちあそびの名前: <%= post.title %></h2>
    <p>対象年齢: <%= post.age_group %></p>
    <p>画像</p>
    <%= attachment_image_tag post, :image, format: 'jpeg', size: '100x100' %>
    <span><%= link_to '詳細を見る', post_path(post) %></span>
  </div>
<% end %>